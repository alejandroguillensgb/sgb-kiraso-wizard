div.modal-header
  h3.modal-title New project
div.modal-body
    div(ng-controller="formsController")
        form(name="modalApp", sf-schema="appSchema",sf-form="appForm",sf-model="appModel", ng-submit="submitApp(modalApp, true)",style="padding:10px;")
    
div.modal-footer
  button.btn.btn-primary(type="button", ng-click="closeModal()") Close
 